package tictactoe;

import baseClasses.Game;
import baseClasses.State;

public class TicTacToeGame extends Game{
	
	public void initializeGame(){
		state = new State();
		state.initializeBoard(3,3);
		
		TicTacToeLogic logic = new TicTacToeLogic(state);
		
		logic.start();
	}
	
	public void start(){
		
	}
	
}
